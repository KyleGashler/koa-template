{
    "name": "koa-api",
    "version": "1.0.0",
    "description": "Basic template to reduce startup friction for koa api apps",
    "main": "dist/index.js",
    "scripts": {
        "test:unit": "mocha -r ts-node/register \"src/**/*.unit.ts\"",
        "lint": "eslint . --ext .ts",
        "lint-and-fix": "eslint . --ext .ts --fix",
        "build-ts": "tsc",
        "serve": "node dist/index.js",
        "build": "npm run lint && tsc",
        "start": "npm run serve"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KyleGashler/typescript-template.git"
    },
    "author": "Kyle Gashler",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/KyleGashler/typescript-templates/issues"
    },
    "homepage": "https://github.com/KyleGashler/typescript-templates#readme",
    "engines": {
        "node": "14.x.x",
        "npm": "6.x.x"
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "koa": "^2.13.1",
        "koa-bodyparser": "^4.3.0",
        "koa-json": "^2.0.2",
        "koa-pino-logger": "^3.0.0",
        "koa-router": "^7.4.0",
        "koa-swagger-decorator": "^1.6.4",
        "pg": "^8.7.1",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.2.37"
    },
    "devDependencies": {
        "@types/chai": "^4.2.20",
        "@types/koa": "^2.13.1",
        "@types/koa__cors": "^3.0.2",
        "@types/koa__router": "^8.0.4",
        "@types/koa-bodyparser": "^4.3.3",
        "@types/koa-json": "^2.0.20",
        "@types/koa-pino-logger": "^3.0.0",
        "@types/koa-router": "^7.4.4",
        "@types/mocha": "^8.2.3",
        "@types/node": "^14.17.11",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "chai": "^4.3.4",
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "mocha": "^9.0.2",
        "nodemon": "^2.0.7",
        "prettier": "^2.3.2",
        "ts-node": "^10.0.0",
        "typescript": "^4.2.3"
